prefix=/usr/local/globus-6
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
bindir=${exec_prefix}/bin
sbindir=${exec_prefix}/sbin
path=${bindir}:${sbindir}

Name: globus-gsi-cert-utils
Description: Globus Toolkit - Globus GSI Cert Utils Library
Version: 9.16
Requires.private: globus-common >= 14, globus-openssl-module >= 3, globus-gsi-openssl-error >= 2, openssl >= 1.0
Libs: -L${libdir} -lglobus_gsi_cert_utils
Cflags: -I${includedir}
